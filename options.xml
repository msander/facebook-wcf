<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE data SYSTEM "http://www.woltlab.com/DTD/options.dtd">
<data>
	<import>
		<categories>
			<category name="facebook">
				<parent>user</parent>
			</category>
		</categories>
			
		<options>
			<option name="module_facebook">
				<categoryname>module.content</categoryname>
				<optiontype>boolean</optiontype>
				<defaultvalue>1</defaultvalue>
			</option>
			<option name="facebook_appid">
				<categoryname>facebook</categoryname>
				<optiontype>text</optiontype>
				<defaultvalue></defaultvalue>
			</option>
			<option name="facebook_secret">
				<categoryname>facebook</categoryname>
				<optiontype>text</optiontype>
				<defaultvalue></defaultvalue>
			</option>
		</options>
	</import>
	
	<delete>
		<category name="facebook.general"/>
		<category name="facebook.general.general"/>

		<option name="facebook_key_public"/>
		<option name="facebook_key_private"/>
	</delete>
</data>
